{
	"manifest_version" : 2,
	"name" : "PageActionNotes",
	"description" : "Note-taking extension implemented using Page-Action",
	"version" : "1.2",
	"page_action" : {
		"default_title" : "PageActionNotes",
		"default_icon" : "icon.png",
		"default_popup" : "popup.html"
	},
	"permissions" : [
		"tabs","declarativeContent"
	],
	"background" : {
		"scripts" : ["event_script.js"],
		"persistent" : false
	}
}