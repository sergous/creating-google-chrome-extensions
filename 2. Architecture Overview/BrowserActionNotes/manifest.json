{
	"manifest_version" : 2,
	"name" : "BrowserActionNotes",
	"description" : "Note-taking extension implemented using Browser-Action",
	"version" : "1.2",
	"browser_action" : {
		"default_title" : "BrowserActionNotes",
		"default_icon" : "icon.png",
		"default_popup" : "popup.html"
	},
	"permissions" : [
		"tabs"
	]
}